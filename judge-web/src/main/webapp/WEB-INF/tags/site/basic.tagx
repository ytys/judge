<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:html="urn:jsptagdir:/WEB-INF/tags/html"
          version="2.0">

    <jsp:directive.tag description="put the tag description here" />
    <jsp:directive.tag trimDirectiveWhitespaces="true"/>

    <!-- The list of normal or fragment attributes can be specified here: -->
    <jsp:directive.attribute name="headBefore" fragment="true"/>
    <jsp:directive.attribute name="head" fragment="true"/>
    <jsp:directive.attribute name="title" fragment="true"/>
    <jsp:directive.attribute name="body" fragment="true" required="true"/>

    <!-- any content can be specified here e.g.: -->
    <html xmlns="http://www.w3.org/1999/xhtml">
        <head>
            <title><jsp:invoke fragment="title"/></title>
            <jsp:invoke fragment="headBefore"/>
            <jsp:invoke fragment="head"/>
        </head>
        <body>
            <jsp:invoke fragment="body"/>
        </body>
    </html>
</jsp:root>
